<h1> Welcome, <%= current_user.username %>!</h1>
<a href="/account/new">Create Payment</a>
<h2> Here are all of your payments </h2>
<ol>
   <% current_user.payments.each do |pay|%>
    <li><a href="/account/<%= pay.id%>"><%=pay.name %></a> Due on: <%= pay.due_date %> </li>
      <% end %>
</ol>
<a href="/logout"> Log out</a>
